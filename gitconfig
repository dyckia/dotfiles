	helper = osxkeychain
[alias]
    st = status
    co = commit
    ck = checkout
    br = branch
    st = add
    ust = reset HEAD
    stage = add
	unstage = reset HEAD
	last = log -1
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
